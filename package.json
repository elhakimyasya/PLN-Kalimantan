    "autoprefixer": "^10.4.2",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-remove-debugger": "^6.9.4",
    "concurrently": "^9.1.0",
    "easy-toggle-state": "^1.16.0",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-babel-minify": "^0.5.2",
    "gulp-clean-css": "^4.3.0",
